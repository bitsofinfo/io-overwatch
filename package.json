{
    "name":"io-overwatch",
    "version":"1.0.0-beta.4",
    "description":"io-event-reactor based daemon for monitoring changes within one or more directories; then reacting by making directories, copying, moving, extracting files or inserting audit records into a database",
    "main":"overwatch.js",
    "directories":{
        "test":"test"
    },
    "scripts":{
        "test":"mocha test/all.js"
    },
    "keywords":[
        "io",
        "event",
        "monitor",
        "changes",
        "alert",
        "watch",
        "file",
        "directory",
        "filesystem",
        "directories"
    ],
    "dependencies": {
        "stateful-process-command-proxy":"latest",
        "mustache":"latest",
        "dateformat":"latest",
        "winston-daily-rotate-file":"latest",
        "winston": "latest",
        "yargs": "latest",
        "io-event-reactor": "latest",
        "io-event-reactor-plugin-chokidar": "latest",
        "io-event-reactor-plugin-mysql": "latest",
        "io-event-reactor-plugin-shell-exec": "latest"
    },
    "devDependencies":{
        "mocha":"latest"
    },
